<% provide(:head_title, I18n.t('activerecord.models.audience_member.other')) %>
<% provide(:page_title, I18n.t('activerecord.models.audience_member.other')) %>
      
<%= render 'shared/search_and_new', new_path: new_admins_audience_member_path,
                                    new_link: 'views.audience_member.links.new',
                                    search_path: search_admins_audience_members_path %>
<div class="col-sm-12 col-lg-3">
  <%= link_to admins_audience_members_import_new_path, class: "btn btn-outline-primary btn-block" do %>
    <%= icon('fas', 'plus', class: 'mr-2') %><%= t('views.audience_member.import.btn_import') %>
  <% end %>
</div>

<div class="text-right mb-4">
  <%= page_entries_info @audience_members %>
</div>

<div class="table-responsive dataTables_wrapper">
  <table class="table card-table table-striped table-vcenter table-options">
    <thead>
      <tr>
        <th><%= t('activerecord.attributes.audience_member.name') %></th>
        <th><%= t('activerecord.attributes.audience_member.cpf') %></th>
        <th><%= t('activerecord.attributes.audience_member.email') %></th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <%= render @audience_members %>
    </tbody>
  </table>

  <%= paginate @audience_members %>
</div>
